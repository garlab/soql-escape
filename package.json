{
  "name": "soql-escape",
  "version": "0.0.2",
  "description": "A template library to escape soql queries",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "author": "Anthony Garcia-Labiad <anthony.garlab@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "test:coverage": "nyc --reporter=html --reporter=text ava",
